{
  "name": "webpack-platzi",
  "version": "1.0.0",
  "description": "Curso Webpack 4 Platzi",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:cli": "webpack --mode development --entry ./index.js --output ./bundle.js",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:css:plugin": "webpack --config ./plugins/webpack.config.js",
    "server": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArnoldRestrepo/webpack-platzi.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@restrepo_arnold",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArnoldRestrepo/webpack-platzi/issues"
  },
  "homepage": "https://github.com/ArnoldRestrepo/webpack-platzi#readme",
  "devDependencies": {
    "css-loader": "3.2.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "style-loader": "1.0.0",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2"
  },
  "dependencies": {
    "express": "4.17.1"
  }
}
